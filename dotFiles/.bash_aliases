" Bash aliases for use on new linux systems

alias la='ls -a'
alias ll='ls -l'
alias rm='rm -I --preserve-root -d -v'
alias venv='virtualenv'
alias venvwrapper='virtualenvwrapper'
alias venvwrapper_start='source /usr/local/bin/virtualenvwrapper.sh;
			 export WORKON_HOME=~/Projects'
alias lsvenvwrapper='lsvirtualenvwrapper'
alias cdvenvwrapper='cdvirtualenvwrapper'
